{
  "name": "trading-platform",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build && node scripts/optimize-build.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:e2e": "playwright test",
    "deploy": "node scripts/deploy.js",
    "deploy:staging": "NODE_ENV=staging npm run deploy",
    "optimize": "node scripts/optimize-build.js",
    "clean": "rimraf .next out dist"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@supabase/supabase-js": "^2.44.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "lucide-react": "^0.400.0",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "clsx": "^2.1.1",
    "react-hook-form": "^7.51.0",
    "react-hot-toast": "^2.4.1",
    "date-fns": "^3.6.0",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.5.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5"
  }
}